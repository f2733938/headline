		<script>
			window.onload = function() {
				auto_data_size();

			};

			function auto_data_size() {
				var imgss = $("figure img");
				$("figure img").each(function() {
					var imgs = new Image();
					imgs.src = $(this).attr("src");
					var w = imgs.width,
						h = imgs.height;
					$(this).parent("a").attr("data-size", "").attr("data-size", w + "x" + h);
				})
			};
		</script>
		<script>
			function jump() {
				//debugger;
				var userId = sanmi.queryParam("userId");
				var cityCode = $("#cityCode").val();
				var param = {
					url: '/headline/publisher/apply/detail',
					data: {
						'userId': userId,
					},
					method: 'post',
					dataType: 'json',
					callback: 'applyStatusHander',
				}
				sanmi.query(param);
			}

			function applyStatusHander(json) {
				var status = json.status;
				//console.info(status);
				if(status == "0") {
					alert(json.msg);
					//sanmi.toPage("/index.html");
					return;
				}
				//	debugger;
				console.info(json);
				//获取用户申请资质信息,判断跳转页面
				var userId = sanmi.queryParam("userId");
				var cityCode = $("#cityCode").val();
				if(json.info == "") {
					$(".pull-right").attr("href", "qualification-apply.html?userId=" + userId + "&area=" + cityCode);
				} else {
					if(json.info.publishTypeFlag == "DOCTOR") {
						$(".pull-right").attr("href", "qualification-apply-result.html?userId=" + userId);
					} else {
						$(".pull-right").attr("href", "operators-qualification-apply-result.html?userId=" + userId);
					}
				}
			}
		</script>
