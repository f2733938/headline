@Data
public class UserInfoParam {
    /**
     * 用户Id请求参数
     */
    private String userid;
    /**
     * 第三方登录微信openId
     */
    private String openId;
    /**
     * 第三方登陆标识
     */
    private String platform;
    /**
     * 身份标识,默认
     */
    private String userFlag;
    /**
     * 用户登陆密码
     */
    private String password;
    /**
     * 用户登录时短信验证码
     */
    private String code;
    /**
     * 用户登录时所用的手机号
     */
    private String phone;

}
